            </tree>
            <parameter id="mutationRate.s:simulated_cognate_data" spec="parameter.RealParameter" name="stateNode">1.0</parameter>
            <parameter id="frequencies.s:simulated_cognate_data" spec="parameter.RealParameter" dimension="2" lower="0.0" name="stateNode" upper="1.0">0.95 0.05</parameter>
            <parameter id="clockRate.c:location" spec="parameter.RealParameter" name="stateNode">1.0</parameter>
            <parameter id="location.location" spec="parameter.RealParameter" dimension="78" minordimension="2" name="stateNode">0.0 0.0</parameter>
            <parameter id="bcov_s.s:simulated_cognate_data" spec="parameter.RealParameter" lower="1.0E-4" name="stateNode" upper="Infinity">0.5</parameter>
        </state>
        <init id="RandomTree.t:tree" spec="beastlabs.evolution.tree.SimpleRandomTree" estimate="false" initial="@Tree.t:tree" taxa="@simulated_cognate_data"/>
        <distribution id="posterior" spec="CompoundDistribution">
            <distribution id="prior" spec="CompoundDistribution">
                <distribution id="YuleModel.t:tree" spec="beast.base.evolution.speciation.YuleModel" tree="@Tree.t:tree">
                    <parameter id="birthRate.t:tree" spec="parameter.RealParameter" estimate="false" name="birthDiffRate">1.0</parameter>
                </distribution>
                <prior id="ClockPrior.c:location" name="distribution" x="@clockRate.c:location">
                    <Uniform id="Uniform.4" name="distr" upper="Infinity"/>
                </prior>
                <prior id="MutationRatePrior.s:simulated_cognate_data" name="distribution" x="@mutationRate.s:simulated_cognate_data">
                    <OneOnX id="OneOnX.0" name="distr"/>
                </prior>
                <distribution id="rootPrior.location" spec="beastclassic.math.distributions.MultivariateNormalDistribution">
                    <parameter id="RealParameter.1" spec="parameter.RealParameter" dimension="2" name="mean">0.0 0.0</parameter>
                    <parameter id="RealParameter.2" spec="parameter.RealParameter" dimension="4" minordimension="2" name="precision">0.001 0.0 0.0 0.001</parameter>
                    <arg id="rootTrait.location" spec="beastclassic.evolution.tree.RootTrait">
                        <traitmap id="traitmap.s:location" spec="beastclassic.evolution.tree.TreeTraitMap" parameter="@location.location" randomizelower="-90 -180" randomizeupper="90 180" traitName="location" tree="@Tree.t:tree">A=-1.55799117750898 2.0932161824307,

B=-4.6192935131144 -3.5289116358993,

