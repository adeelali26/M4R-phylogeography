            <log idref="likelihood"/>
            <log idref="prior"/>
        </logger>
        <logger id="treelog.t:tree" spec="Logger" fileName="simulation-study-GEO.trees" logEvery="1000" mode="tree">
            <log id="TreeWithMetaDataLogger.t:tree" spec="beast.base.evolution.TreeWithMetaDataLogger" tree="@Tree.t:tree">
                <metadata idref="location.location"/>
            </log>
        </logger>
