        <operator id="YuleModelWide.t:tree" spec="Exchange" isNarrow="false" tree="@Tree.t:tree" weight="3.0"/>
        <operator id="YuleModelWilsonBalding.t:tree" spec="WilsonBalding" tree="@Tree.t:tree" weight="3.0"/>
        <operator id="traitGibbsOperator.location" spec="beastclassic.evolution.operators.TraitGibbsOperator" likelihood="@locationtreeLikelihood.location" precisionMatrix="@precisionMatrix.s:location" traitmap="@traitmap.s:location" tree="@Tree.t:tree" weight="50.0"/>
        <operator id="RootTraitRandowWalkOperator.location" spec="beastclassic.evolution.operators.RootTraitRandowWalkOperator" parameter="@location.location" traitmap="@traitmap.s:location" weight="5.0" windowSize="10.0"/>
        <operator id="bcovSwitchParamScaler.s:simulated_cognate_data" spec="ScaleOperator" parameter="@bcov_s.s:simulated_cognate_data" weight="0.1"/>
        <operator id="StrictClockRateScaler.c:location" spec="ScaleOperator" parameter="@clockRate.c:location" weight="3.0"/>
        <logger id="tracelog" spec="Logger" fileName="simulation-study-GEO.log" logEvery="1000" model="@posterior" sort="smart">
            <log idref="posterior"/>
            <log idref="likelihood"/>
            <log idref="prior"/>
            <log idref="treeLikelihood.simulated_cognate_data"/>
            <log id="TreeHeight.t:tree" spec="beast.base.evolution.tree.TreeStatLogger" tree="@Tree.t:tree"/>
            <log idref="mutationRate.s:simulated_cognate_data"/>
            <log idref="YuleModel.t:tree"/>
            <log idref="frequencies.s:simulated_cognate_data"/>
            <log idref="clockRate.c:location"/>
            <log idref="locationtreeLikelihood.location"/>
            <log id="rateStat.location" spec="beast.base.evolution.RateStatistic" tree="@Tree.t:tree" treeLikelihood="@locationtreeLikelihood.location"/>
            <log idref="rootTrait.location"/>
            <log idref="bcov_s.s:simulated_cognate_data"/>
        </logger>
        <logger id="screenlog" spec="Logger" logEvery="1000">
            <log idref="posterior"/>
