T=-1.72064279792949 1.85585540689245</traitmap>
                    </arg>
                </distribution>
                <prior id="bcov_s_prior.s:simulated_cognate_data" name="distribution" x="@bcov_s.s:simulated_cognate_data">
                    <Gamma id="Gamma.0" name="distr">
                        <alpha id="Function$Constant.0" spec="Function$Constant" value="0.05"/>
                        <beta id="Function$Constant.1" spec="Function$Constant" value="10.0"/>
                    </Gamma>
                </prior>
            </distribution>
            <distribution id="likelihood" spec="CompoundDistribution" useThreads="true">
                <distribution id="treeLikelihood.simulated_cognate_data" spec="TreeLikelihood" tree="@Tree.t:tree" useAmbiguities="true">
                    <data
id="orgdata.simulated_cognate_data"
spec="FilteredAlignment"
ascertained="true"
data="@simulated_cognate_data"
excludeto="1"
filter="-">
                        <userDataType id="TwoStateCovarion.0" spec="beast.base.evolution.datatype.TwoStateCovarion"/>
                    </data>
                    <siteModel id="SiteModel.s:simulated_cognate_data" spec="SiteModel" gammaCategoryCount="1" mutationRate="@mutationRate.s:simulated_cognate_data">
                        <parameter id="gammaShape.s:simulated_cognate_data" spec="parameter.RealParameter" estimate="false" name="shape">1.0</parameter>
                        <parameter id="proportionInvariant.s:simulated_cognate_data" spec="parameter.RealParameter" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                        <substModel id="covarion" spec="BinaryCovarion" switchRate="@bcov_s.s:simulated_cognate_data" vfrequencies="@frequencies.s:simulated_cognate_data">
                            <parameter id="bcov_alpha.s:simulated_cognate_data" spec="parameter.RealParameter" estimate="false" lower="1.0E-4" name="alpha" upper="1.0">0.1</parameter>
                            <parameter id="hiddenfrequencies.s:simulated_cognate_data" spec="parameter.RealParameter" dimension="2" estimate="false" lower="0.0" name="hfrequencies" upper="1.0">0.5 0.5</parameter>
                            <frequencies id="dummyfrequencies.s:simulated_cognate_data" spec="Frequencies" data="@simulated_cognate_data" estimate="false"/>
                        </substModel>
                    </siteModel>
                    <branchRateModel id="StrictClock.c:clock" spec="beast.base.evolution.branchratemodel.StrictClockModel">
                        <parameter id="clockRate.c:clock" spec="parameter.RealParameter" estimate="false" name="clock.rate">1.0</parameter>
                    </branchRateModel>
                </distribution>
                <distribution id="locationtreeLikelihood.location" spec="beastclassic.continuous.SampledMultivariateTraitLikelihood" reciprocalRates="true" scaleByTime="true" traitParameter="@location.location" tree="@Tree.t:tree" useTreeLength="true">
